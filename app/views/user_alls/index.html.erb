<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<%= link_to "New user all", new_user_all_path, data: { controller: 'user-model' }, class: "btn btn-primary" %>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Last Name</th>
      <th>Address</th>
      <th>City</th>
      <th>Contact No.</th>
      <th>State</th>
      <th>Actions</th>
    </tr>
  </thead>
  
  <tbody id="user_list">
    <% @user_alls.each do |user_all| %>
      <%= render "user", user_all: user_all  %>
    <% end %>  
  </tbody>
</table>  

